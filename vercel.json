{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    },
    {
      "src": "audio/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/audio/(.*)",
      "dest": "/audio/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}

